{
  "name": "@oyen-o/ng-multiselect-dropdown-zoneless",
  "version": "1.0.0",
  "description": "Angular Multi-Select Dropdown with Zoneless Support for Angular 17+",
  "author": "Nilesh Patel",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201 --verbose",
    "build": "ng build",
    "ng:test": "ng test",
    "lint": "ng lint",
    "test": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "build:prod": "ng build --prod --base-href https://nileshpatel17.github.io/ng-multiselect-dropdown/",
    "clear:lib": "rimraf dist-lib",
    "postcopyfiles": "copyfiles -u 1 ./dist-lib/**/*.* node_modules/ng-multiselect-dropdown",
    "copyfiles": "mkdir dist-lib/themes && copyfiles ng-multiselect-dropdown.theme.scss dist-lib/themes",
    "build:lib": "npm run clear:lib && ng-packagr -p ng-package.json",
    "postbuild:lib": "npm run copyfiles",
    "prepublish:lib": "npm run build:lib",
    "publish:lib": "cd dist-lib && npm publish --access public",
    "publish:dry-run": "cd dist-lib && npm publish --dry-run --access public",
    "version:patch": "npm version patch && npm run publish:lib",
    "version:minor": "npm version minor && npm run publish:lib",
    "version:major": "npm version major && npm run publish:lib",
    "prepublish": "npm run build",
    "publish": "ngh --no-silent false --name=\"nileshpatel17\" --email=\"nilesh.nvs@hotmail.com\"",
    "deploy": "ng build --prod --base-href /ng-multiselect-dropdown/ && npm run deployOnly",
    "deployOnly": "angular-cli-ghpages --no-silent --repo=https://github.com/NileshPatel17/ng-multiselect-dropdown.git --name=\"Nilesh Patel\" --email=nilesh.nvs@hotmail.com",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "keywords": [
    "angular",
    "angular17",
    "angular18",
    "angular19",
    "angular20",
    "multiselect",
    "dropdown",
    "zoneless",
    "signals",
    "standalone",
    "ng-multiselect-dropdown",
    "angular multiselect dropdown",
    "signals angular multiselect dropdown",
    "angular20 multiselect dropdown",
    "ng multiselect dropdown",
    "ng20 multiselect dropdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Oyen-o/ng-multiselect-dropdown-Zoneless"
  },
  "bugs": {
    "url": "https://github.com/Oyen-o/ng-multiselect-dropdown-Zoneless/issues"
  },
  "homepage": "https://github.com/Oyen-o/ng-multiselect-dropdown-Zoneless#readme",
  "main": "bundles/ng-multiselect-dropdown.umd.js",
  "module": "fesm2022/ng-multiselect-dropdown.mjs",
  "typings": "index.d.ts",
  "peerDependencies": {
    "@angular/core": ">=17.0.0",
    "@angular/common": ">=17.0.0",
    "@angular/forms": ">=17.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.1",
    "@angular/cli": "^20.3.1",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/language-service": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-browser-dynamic": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@types/jasmine": "^5.1.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.0-rc.2",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.0.0",
    "core-js": "^3.2.1",
    "jasmine-core": "^5.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.1.2",
    "ngx-bootstrap": "^20.0.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@angular/build": "^20.3.1",
    "tslib": "^2.8.1"
  }
}
